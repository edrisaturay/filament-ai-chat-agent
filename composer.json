{
    "name": "edrisaturay/filament-ai-chat-agent",
    "description": "Integrate with OpenAI ChatGPT",
    "version": "2.0.8",
    "keywords": [
        "edrisaturay",
        "laravel",
        "filament-ai-chat-agent"
    ],
    "homepage": "https://github.com/edrisaturay/filament-ai-chat-agent",
    "license": "MIT",
    "require": {
        "php": "^8.3",
        "spatie/laravel-package-tools": "^1.9.2",
        "illuminate/contracts": "^12.0",
        "filament/filament": "^5.0",
        "livewire/livewire": "^4.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^8.1",
        "orchestra/testbench": "^10.0",
        "pestphp/pest": "^3.0",
        "pestphp/pest-plugin-arch": "^3.0",
        "pestphp/pest-plugin-laravel": "^3.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "EdrisaTuray\\FilamentAiChatAgent\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EdrisaTuray\\FilamentAiChatAgent\\Tests\\": "tests"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "EdrisaTuray\\FilamentAiChatAgent\\FilamentAiChatAgentServiceProvider"
            ],
            "aliases": {
                "FilamentAiChatAgent": "EdrisaTuray\\FilamentAiChatAgent\\Facades\\FilamentAiChatAgent"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
