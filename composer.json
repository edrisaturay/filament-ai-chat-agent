{
    "name": "edrisaturay/filament-ai-chat-agent",
    "description": "A Filament plugin that allows you to easily integrate AI chat capabilities into your Filament project",
    "version": "1.0.5",
    "keywords": [
        "likeabas",
        "edrisaturay",
        "laravel",
        "filament-ai-chat-agent"
    ],
    "homepage": "https://github.com/edrisaturay/filament-ai-chat-agent",
    "license": "MIT",
    "authors": [
        {
            "name": "Edrisa A Turay",
            "email": "edrisa@edrisa.com",
            "homepage": "https://github.com/edrisaturay",
            "role": "Maintainer"
        },
        {
            "name": "Bas Schleijpen",
            "homepage": "https://github.com/likeabas",
            "role": "Original Creator"
        }
    ],
    "require": {
        "php": ">=8.0",
        "spatie/laravel-package-tools": "^1.9.2",
        "illuminate/contracts": "^9.0|^10.0|^11.0",
        "livewire/livewire": "^3.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.9|^8.1",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "EdrisaTuray\\FilamentAiChatAgent\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EdrisaTuray\\FilamentAiChatAgent\\Tests\\": "tests"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "EdrisaTuray\\FilamentAiChatAgent\\FilamentAiChatAgentServiceProvider"
            ],
            "aliases": {
                "FilamentAiChatAgent": "EdrisaTuray\\FilamentAiChatAgent\\Facades\\FilamentAiChatAgent"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}